# ConsenTide Environment Variables

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Backend Configuration
PORT=3001
NODE_ENV=development
JWT_SECRET=your_jwt_secret_here

# Frontend Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_METAGRAPH_URL=http://localhost:9200

# Constellation Network Configuration
CONSTELLATION_NODE_URL=http://localhost:9200
CONSTELLATION_NETWORK_ID=integrationnet
CONSTELLATION_WALLET_ADDRESS=your_wallet_address
CONSTELLATION_PRIVATE_KEY=your_private_key_hex
CONSTELLATION_PUBLIC_KEY=your_public_key_hex

# Certificate Configuration
CERTIFICATE_PATH=./p12-files/node1.p12
CERTIFICATE_PASSWORD=your_certificate_password
CERTIFICATE_ALIAS=node1

# Railway Configuration (for deployment)
RAILWAY_STATIC_URL=your_railway_static_url
DATABASE_URL=your_postgresql_connection_string